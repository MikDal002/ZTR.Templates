{
  "$schema": "http://json.schemastore.org/template",
  "author": "Zaprogramuj To Raz!",
  "classifications": [
    "Common",
    "Component"
  ],
  "identity": "ZTR.ComponentTemplate.CSharp",
  "name": "Program It Once core template",
  "shortName": "ztr-component",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "PersonComponent",
  "preferNameDirectory": true,
  "baselines": {
    "app": {
      "description": "Target .NET 5.0",
      "defaultOverrides": {
        "Framework": "net5.0"
      }
    },
    "standard": {
      "description": "Target netstandard 2.0",
      "defaultOverrides": {
        "Framework": "netstandard2.0"
      }
    }
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for this project",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net5.0",
          "description": "Target net5.0",
          "displayName": ".NET 5.0"
        },
        {
          "choice": "netstandard2.1",
          "description": "Target netstandard2.1",
          "displayName": ".NET Standard 2.1"
        },
        {
          "choice": "netstandard2.0",
          "description": "Target netstandard2.0",
          "displayName": ".NET Standard 2.0"
        }
      ],
      "replaces": "net5.0",
      "defaultValue": "net5.0",
      "displayName": "Framework"
    }
  }
}