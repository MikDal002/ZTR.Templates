{
    "$schema": "http://json.schemastore.org/template",
    "author": "Zaprogramuj To Raz!",
    "classifications": [ "Common", "Console" ],
    "identity": "ZTR.ConsoleTemplate.CSharp",
    "name": "Program It Once Console template",
    "shortName": "ztr-console",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "ConsoleTemplate",
    "baselines": {
      "app": {
        "description": "Target .NET 6.0",
        "defaultOverrides": {
          "Framework": "net6.0"
        }
      },
      "standard": {
        "description": "Target netstandard 2.0",
        "defaultOverrides": {
          "Framework": "netstandard2.0"
        }
      }
    },
    "symbols": {
      "Framework": {
        "type": "parameter",
        "description": "The target framework for this project",
        "datatype": "choice",
        "choices": [
          {
            "choice": "net6.0",
            "description": "Target net6.0",
            "displayName": ".NET 6.0"
          },
          {
            "choice": "net5.0",
            "description": "Target net5.0",
            "displayName": ".NET 5.0"
          },
          {
            "choice": "netstandard2.1",
            "description": "Target netstandard2.1",
            "displayName": ".NET Standard 2.1"
          },
          {
            "choice": "netstandard2.0",
            "description": "Target netstandard2.0",
            "displayName": ".NET Standard 2.0"
          }
        ],
        "replaces": "net5.0",
        "defaultValue": "net5.0",
        "displayName": "Framework"
      }
    }
  }